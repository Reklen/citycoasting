<% @stamps.each do |stamp| %>
  <div class="box panel panel-default">
    <div class = "center">
      <%= link_to image_tag(stamp.image.url), stamp %>
      <div class = "panel-body">
        <%= stamp.description %><br>
        <%= render 'stamps/neighborhood_key', :stamp => stamp %>
        <p><strong><%= stamp.user.username if stamp.user %></strong></p>
        <div class = "pull-left">
          <%= render 'stamps/pricerange_key', :stamp => stamp %>
        </div>
        <div class = "pull-right">
          <%= render 'stamps/category_key', :stamp => stamp %>
        </div>
      </div>
    </div>
  </div>
<% end %>