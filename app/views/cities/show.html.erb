<div class = "row">
	<div class ="col-md-12">
<h1><%= @city.name %></h1>
<div>
	<ul class ="form-group"><%= select_tag "Categories",options_from_collection_for_select(Category.all, 'id', 'name', params[:category_id]), prompt:"Everything", id: "category_select", class: "form-control btn dropdown-toggle" %>
	</ul>
</div>
	</div>
</div>
<% if @stamps %>
	<div id="stamps" class="transitions-enabled">	
  	<% @stamps.each do |stamp| %>
      <div class="box panel panel-default">
        <div class = "center">
          <%= link_to image_tag(stamp.image.url(:medium)), stamp %>
          <div class = "panel-body">
            <div class = "pull-left text-left">
              <p><strong><%= stamp.name if stamp.name %></strong></p>
              <p><%= stamp.category.name if stamp.category %></p>
            </div>
            <div class = "pull-right">
              <%= stamp.neighborhood.name if stamp.neighborhood %>
              by <%= link_to stamp.user.username, profile_path(stamp.user.username) if stamp.user %>
            </div>
          </div>
        </div>
      </div>
    <% end %>
	</div>

<% end %>
