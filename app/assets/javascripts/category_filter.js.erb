$(function(){
  $('#category_select').on('change', function(){
    var category_id = $(this).val();
    window.location = "<%= ENV['RAILS_RELATIVE_URL_ROOT'] %>/" + "category/" + category_id;

  });
});


$(function(){
  $('#category_city_select').on('change', function(){
    var category_id = $(this).val();
    window.location = "<%= ENV['RAILS_RELATIVE_URL_ROOT'] %>/" + "cities/" + gon.city_instance_id + "/" + category_id;
  });
});
